{
  "version": "2.0",
  "approach": "semantic_clustering",
  "steps": [
    {
      "step": 1,
      "name": "Capture",
      "description": "Get response and context tokens from source model",
      "data": [
        "response_text",
        "context_tokens",
        "embedding_vector"
      ]
    },
    {
      "step": 2,
      "name": "Analyze",
      "description": "Extract semantic clusters and key concepts",
      "data": [
        "semantic_clusters",
        "concept_anchors",
        "theme_map"
      ]
    },
    {
      "step": 3,
      "name": "Compress",
      "description": "Create compressed semantic representation",
      "data": [
        "anchor_notation",
        "cluster_weights",
        "concept_graph"
      ]
    },
    {
      "step": 4,
      "name": "Transfer",
      "description": "Reconstruct in target model vocabulary",
      "data": [
        "translated_prompt",
        "semantic_bridges",
        "context_injection"
      ]
    },
    {
      "step": 5,
      "name": "Verify",
      "description": "Check theme persistence and coherence",
      "data": [
        "theme_overlap",
        "coherence_score",
        "drift_metrics"
      ]
    }
  ],
  "semantic_bridges": {
